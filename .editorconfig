root = true

[*]
end_of_line = lf
indent_style = space
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 120

[{go.mod,go.sum,*.go}]
indent_style = tab
indent_size = 4

[*.ps1]
end_of_line = crlf
indent_size = 4

[*.md]
trim_trailing_whitespace = false

[*.xml]
insert_final_newline = false

[{token.txt,iscc}]
insert_final_newline = false
